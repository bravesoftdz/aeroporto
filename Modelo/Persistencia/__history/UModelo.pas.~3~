unit UModelo;

interface

uses
    UEntidade
  , UMarca
  ;

type
  TMODELO = class(TENTIDADE)
  public
    NOME : String;
    MARCA: TMARCA;

    constructor Create; override;
    destructor Detroy; override;
  end;

const
  TBL_MODELO          = 'MODELO';
  FLD_MODELO_NOME     = 'NOME';
  FLD_MODELO_ID_MARCA = 'ID_MARCA';

implementation

uses
    SysUtils
  ;

{ TMODELO }

constructor TMODELO.Create;
begin
  inherited;
  MARCA := TMARCA.Create;
end;

destructor TMODELO.Detroy;
begin
  FreeAndNil(MARCA);
  inherited;
end;

end.
