unit UAeronave;

interface

uses
    UEntidade
  , UModelo
  ;

type
  TAERONAVE = class(TENTIDADE)
  public
    PREFIXO            : String;
    MODELO             : TMODELO;
    AUTOMONIA_VOO_NM   : Integer;
    ASSENTOS           : Integer;
    PISTA_NECESSARIA_KM: Double;

    constructor Create;
    destructor Destroy;
  end;

implementation

uses
    SysUtils
  ;

{ TAERONAVE }

constructor TAERONAVE.Create;
begin
  MODELO := TMODELO.Create;
end;

destructor TAERONAVE.Destroy;
begin
  FreeAndNil(MODELO);
end;

end.
