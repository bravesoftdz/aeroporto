unit UEstado;

interface

type
  TESTADO = class
  private
    FId  : Integer;
    FNome: String;
    FUF  : String;

  public
    property ID  : Integer read FId   write FId;
    property NOME: String  read FNome write FNome;
    property UF  : String  read FUF   write FUF;
  end;

const
  TBL_ESTADO      = 'ESTADO';
  FLD_ESTADO_ID   = 'ID';
  FLD_ESTADO_NOME = 'NOME';
  FLD_ESTADO_UF   = 'UF';

  VW_ESTADO     = 'VW_ESTADO';
  FLD_VW_SIGLA  = 'Sigla';
  FLD_VW_ESTADO = 'Id.';

implementation

end.
