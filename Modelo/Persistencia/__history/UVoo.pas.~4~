unit UVoo;

interface

uses
    UEntidade
  , URota
  , UAeronave
  ;

type
  TVOO = class(TENTIDADE)
  public
    ROTA    : TROTA;
    AERONAVE: TAERONAVE;
    PARTIDA : TDateTime;
    CHEDADA : TDateTime;

    constructor Create; override;
    destructor Destroy; override;
  end;

implementation

uses
    SysUtils
  ;

{ TVOO }

constructor TVOO.Create;
begin
  ROTA     := TROTA.Create;
  AERONAVE := TAERONAVE.Create;
end;

destructor TVOO.Destroy;
begin
  FreeAndNil(ROTA);
  FreeAndNil(AERONAVE);
  inherited;
end;

end.
